{
  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "allowJs": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "experimentalDecorators": true,
    "typeRoots": ["node_modules/@types", "src/js/@types"]
  },
  "exclude": ["node_modules", "src/js/libs"],
  "include": ["src/js/**/*"]
}
